<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bck8">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/bck8"
        android:layout_gravity="center_horizontal">

        <ImageView
            android:layout_width="90dp"
            android:layout_height="100dp"
            android:background="@drawable/transport22"
            android:scaleType="center"
            android:layout_marginLeft="150dp"
            android:id="@+id/imageView9" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="21dp"

        android:background="@drawable/bck8"
        android:layout_gravity="center_horizontal">
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="@drawable/bck8"
        android:layout_gravity="center_horizontal">

        <ImageView
            android:layout_width="90dp"
            android:layout_marginLeft="70dp"
            android:layout_height="100dp"
            android:id="@+id/imageView10"
            android:background="@drawable/doctor22"/>

        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:layout_marginLeft="85dp"
            android:id="@+id/imageView11"
            android:background="@drawable/nurse23"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="21dp"

        android:background="@drawable/bck8"
        android:layout_gravity="center_horizontal">
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="@drawable/bck8"
        android:layout_gravity="center_horizontal">

        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:layout_marginLeft="5dp"
            android:id="@+id/imageView12"
            android:background="@drawable/medicine22"/>

        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:id="@+id/imageView13"
            android:layout_marginLeft="65dp"
            android:scaleType="center"
            android:background="@drawable/userred"/>

        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:id="@+id/imageView14"
            android:background="@drawable/microscop22"
            android:layout_marginLeft="55dp"/>


    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="21dp"

        android:background="@drawable/bck8"
        android:layout_gravity="center_horizontal">
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/bck8"
        android:layout_gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="FEELING LAZY?"
            android:textColor="#FFFFFF"
            android:layout_marginLeft="140dp"
            android:id="@+id/textView9" />
    </LinearLayout>

    <Button
        android:layout_width="170dp"
        android:layout_height="60dp"
        android:text="CALL ME BACK"
        android:id="@+id/button4"
        android:background="#99800000"
        android:layout_gravity="center_horizontal" />
</LinearLayout>