<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>healthokapp</display-name>
  <servlet>
    <servlet-name>Jersey REST Service</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>jersey.config.server.provider.packages</param-name>
      <param-value>restapi</param-value>
    </init-param>
    
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Jersey REST Service</servlet-name>
    <url-pattern>/rest/*</url-pattern>
  </servlet-mapping>
  <env-entry> 
    <env-entry-name>server</env-entry-name> 
    <env-entry-type>java.lang.String</env-entry-type> 
    <env-entry-value>localhost</env-entry-value> 
  </env-entry> 
  <env-entry> 
    <env-entry-name>port</env-entry-name> 
    <env-entry-type>java.lang.String</env-entry-type> 
    <env-entry-value>3306</env-entry-value> 
  </env-entry> 
  <env-entry> 
    <env-entry-name>dbname</env-entry-name> 
    <env-entry-type>java.lang.String</env-entry-type> 
    <env-entry-value>healthok</env-entry-value> 
  </env-entry> 
  <env-entry> 
    <env-entry-name>user</env-entry-name> 
    <env-entry-type>java.lang.String</env-entry-type> 
    <env-entry-value>root</env-entry-value> 
  </env-entry> 
  <env-entry> 
    <env-entry-name>password</env-entry-name> 
    <env-entry-type>java.lang.String</env-entry-type> 
    <env-entry-value>root</env-entry-value> 
</env-entry>




  <context-param> 
    <description>server</description> 
    <param-name>server</param-name> 
    <param-value>localhost</param-value> 
  </context-param> 
  <context-param> 
    <description>port</description> 
    <param-name>port</param-name> 
    <param-value>3306</param-value> 
  </context-param> 
  <context-param> 
    <description>dbname</description> 
    <param-name>dbname</param-name> 
    <param-value>healthok</param-value> 
  </context-param> 
  <context-param> 
    <description>user</description> 
    <param-name>user</param-name> 
    <param-value>root</param-value> 
  </context-param> 
  <context-param> 
    <description>password</description> 
    <param-name>password</param-name> 
    <param-value>root</param-value> 
  </context-param> 



</web-app>